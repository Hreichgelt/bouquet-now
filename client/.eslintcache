e(\"Flower Shop\");\n  }, []);\n\n  // Changes document title according to pathname\n  useEffect(() => {\n    document.title = page;\n  }, [page]);\n\n  return (\n    <ApolloProvider client={client}>\n      <Router>\n      <StoreProvider>\n        <div id='wholePageContainer'>\n          <Header page={page} setPage={setPage}/>\n          <div id='mainSection' style={{flexGrow: \"1\"}}>\n            <Routes>\n              <Route path=\"/\" element={<Home />} />\n              <Route path=\"/login\" element={<Login />} />\n              <Route path=\"/signup\" element={<Signup />} />\n              <Route path=\"/me\" element={<Profile />} />\n              <Route path=\"/users/:id\" element={<Profile />} />\n              <Route path=\"/occasion\" element={<Occasion />} />\n              <Route path=\"/occasion/:id\" element={<BouquetList />} />\n              <Route path=\"/bouquet/:id\" element={<Bouquet />} />\n              <Route path=\"/contact\" element={<Contact />} />\n              <Route path=\"/cart\" element={<Cart />} />\n              <Route path=\"/success\" element={<Success />} />\n            </Routes>\n          </div>\n          <Footer />\n        </div>\n      </StoreProvider>\n      </Router>\n    </ApolloProvider>\n  );\n}\n\nexport default App;\n","/Users/hansreichgelt/Bootcamp/Homework/flower-shop/client/src/pages/Contact.js",[],"/Users/hansreichgelt/Bootcamp/Homework/flower-shop/client/src/pages/Success.js",[],"/Users/hansreichgelt/Bootcamp/Homework/flower-shop/client/src/pages/Home.js",[],"/Users/hansreichgelt/Bootcamp/Homework/flower-shop/client/src/pages/Profile.js",[],"/Users/hansreichgelt/Bootcamp/Homework/flower-shop/client/src/utils/GlobalState.js",[],"/Users/hansreichgelt/Bootcamp/Homework/flower-shop/client/src/components/Header/index.js",["126","127","128"],"import React, {useEffect} from 'react';\nimport Navbar from '../Navbar';\nimport { Link } from \"react-router-dom\"\nimport { FaShoppingCart } from \"react-icons/fa\"\nimpor