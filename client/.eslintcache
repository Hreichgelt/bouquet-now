],"import React, { useState } from 'react';\nimport { Link } from 'react-router-dom';\nimport { useMutation } from '@apollo/client';\nimport { LOGIN_USER } from '../utils/mutations';\nimport Header from '../components/Header';\nimport Navbar from \"../components/Navbar/index\"\n\n\nimport Auth from '../utils/auth';\n\nconst Login = () => {\n  const [formState, setFormState] = useState({ email: '', password: '' });\n  const [login, { error, data }] = useMutation(LOGIN_USER);\n\n  // update state based on form input changes\n  const handleChange = (event) => {\n    const { name, value } = event.target;\n\n    setFormState({\n      ...formState,\n      [name]: value,\n    });\n  };\n\n  // submit form\n  const handleFormSubmit = async (event) => {\n    event.preventDefault();\n    try {\n      const { data } = await login({\n        variables: { ...formState },\n      });\n\n      Auth.login(data.login.token);\n    } catch (e) {\n      console.error(e);\n    }\n\n    // clear form values\n    setFormState({\n      email: '',\n      password: '',\n    });\n  };\n\n  const renderForm = () => {\n    if (data) {\n      return (\n        <p>\n          Success! You may now head{' '}\n          <Link to=\"/\">back to the homepage.</Link>\n        </p>\n      )\n    } \n    return (\n      <>\n      <form style={{paddingTop: \"4rem\"}}onSubmit={handleFormSubmit}>\n        <input\n          placeholder=\"Your email\"\n          name=\"email\"\n          type=\"email\"\n          value={formState.email}\n          onChange={handleChange}\n        />\n        <input\n          placeholder=\"******\"\n          name=\"password\"\n          type=\"password\"\n          value={formState.password}\n          onChange={handleChange}\n        />\n        <button type=\"submit\">\n          Submit\n        </button>\n      </form>\n    </>\n    );\n  };\n\n  return (\n    <main style={{paddingTop: \"4rem\"}}>\n      <h4>Login</h4>\n      <div>\n        {renderForm()}\n        {error && <div>{error.message}</div>}\n      </div>\n    </main>\n  );\n};\n\nexport default Login;\n","C:\\Users\\ntrau\\Bootcamp\\projects\\flower-shop\\client\\src\\pages\\Profile.js",[],"C:\\Users\\ntrau\\Bootcamp\\projects\\flower-shop\\client\\src\\components\\Footer\\index.js",["108"],"C:\\Users\\ntrau\\Bootcamp\\projects\\flower-shop\\client\\src\\components\\Header\\index.js",["109"],"C:\\Users\\ntrau\\Bootcamp\\projects\\flower-shop\\client\\src\\utils\\mutations.js",[],"C:\\Users\\ntrau\\Bootcamp\\projects\\flower-shop\\client\\src\\utils\\auth.js",[],"C:\\Users\\ntrau\\Bootcamp\\projects\\flower-shop\\client\\src\\utils\\queries.js",[],"C:\\Users\\ntrau\\Bootcamp\\projects\\flower-shop\\client\\src\\components\\UserList\\index.js",[],"C:\\Users\\ntrau\\Bootcamp\\projects\\flower-shop\\client\\src\\components\\Navbar\\index.js",[],"C:\\Users\\ntrau\\Bootcamp\\projects\\flower-shop\\client\\src\\pages\\Occasion.js",[],"C:\\Users\\ntrau\\Bootcamp\\projects\\flower-shop\\client\\src\\pages\\Bouquet.js",[],"C:\\Users\\ntrau\\Bootcamp\\projects\\flower-shop\\client\\src\\pages\\Contact.js",["110"],"C:\\Users\\ntrau\\Bootcamp\\projects\\flower-shop\\client\\src\\pages\\ContactForm.js",[],"C:\\Users\\ntrau\\Bootcamp\\projects\\flower-shop\\client\\src\\components\\FeaturedItemsList\\index.js",["111","112","113"],"C:\\Users\\ntrau\\Bootcamp\\projects\\flower-shop\\client\\src\\components\\FeaturedItems\\index.js",[],"C:\\Users\\ntrau\\Bootcamp\\projects\\flower-shop\\client\\src\\utils\\GlobalState.js",[],"C:\\Users\\ntrau\\Bootcamp\\projects\\flower-shop\\client\\src\\utils\\helpers.js",[],"C:\\Users\\ntrau\\Bootcamp\\projects\\flower-shop\\client\\src\\utils\\actions.js",[],"C:\\Users\\ntrau\\Bootcamp\\projects\\flower-shop\\client\\src\\utils\\reducers.js",[],{"ruleId":"114","replacedBy":"115"},{"ruleId":"116","replacedBy":"117"},{"ruleId":"118","severity":1,"message":"119","line":17,"column":8,"nodeType":"120","messageId":"121","endLine":17,"endColumn":14},{"ruleId":"