ch((product) => {\n            idbPromise('products', 'put', product);\n          });\n        } else if (!loading) {\n          idbPromise('products', 'get').then((products) => {\n            dispatch({\n              type: QUERY_FEATURED,\n              products: products,\n            });\n          });\n        }\n      }, [data, loading, dispatch]);\n    if(loading){\n        return (\n            <div>\n                <h2>Loading...</h2>\n            </div>\n        )\n    }\n    return (\n        <div>\n            <h2>Featured Items:</h2>\n            {data?.featured.map((item) => (\n                    <FeaturedItems \n                        key={item._id}\n                        _id={item._id}\n                        image={item.image}\n                        name={item.name}\n                        price={item.price}\n                        />\n                )) ||\" No items to display\"}\n        </div>\n    )\n}\n\nexport default FeaturedItemsList","/Users/hansreichgelt/Bootcamp/Homework/flower-shop/client/src/utils/actions.js",[],"/Users/hansreichgelt/Bootcamp/Homework/flower-shop/client/src/utils/helpers.js",[],"/Users/hansreichgelt/Bootcamp/Homework/flower-shop/client/src/components/FeaturedItems/index.js",[],{"ruleId":"113","replacedBy":"114"},{"ruleId":"115","replacedBy":"116"},{"ruleId":"117","severity":1,"message":"118","line":17,"column":8,"nodeType":"119","messageId":"120","endLine":17,"endColumn":14},{"ruleId":"117","severity":1,"message":"121","line":1,"column":17,"nodeType":"119","messageId":"120","endLine":1,"endColumn":25},{"ruleId":"117","severity":1,"message":"122","line":5,"column":8,"nodeType":"119","messageId":"120","endLine":5,"endColumn":12},{"ruleId":"117","severity":1,"message":"123","line":12,"column":11,"nodeType":"119","messageId":"120","endLine":12,"endColumn":18},{"ruleId":"117","severity":1,"message":"124","line":13,"column":9,"nodeType":"119","messageId":"120","endLine":13,"endColumn":14},{"ruleId":"117","severity"