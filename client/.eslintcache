 else if (path === \"/contact\" && page !==\"Flower Shop | Contact\") setPage(\"Flower Shop | Contact Us\");\r\n    else if (path === \"/cart\" && page !==\"Flower Shop | Checkout\") setPage(\"Flower Shop | Checkout\");\r\n    else if (path === \"/success\" && page !==\"Flower Shop | Success\") setPage(\"Flower Shop | Success\");\r\n    else if (page !==\"Flower Shop\") setPage(\"Flower Shop\");\r\n  }, []);\r\n\r\n  // Changes document title according to pathname\r\n  useEffect(() => {\r\n    document.title = page;\r\n  }, [page]);\r\n\r\n  return (\r\n    <ApolloProvider client={client}>\r\n      <Router>\r\n      <StoreProvider>\r\n        <div id='wholePageContainer'>\r\n          <Header page={page} setPage={setPage}/>\r\n          <div id='mainSection' style={{flexGrow: \"1\"}}>\r\n            <Routes>\r\n              <Route path=\"/\" element={<Home />} />\r\n              <Route path=\"/login\" element={<Login />} />\r\n              <Route path=\"/signup\" element={<Signup />} />\r\n              <Route path=\"/me\" element={<Profile />} />\r\n              <Route path=\"/users/:id\" element={<Profile />} />\r\n              <Route path=\"/occasion\" element={<Occasion />} />\r\n              <Route path=\"/occasion/:id\" element={<BouquetList />} />\r\n              <Route path=\"/bouquet/:id\" element={<Bouquet />} />\r\n              <Route path=\"/contact\" element={<Contact />} />\r\n              <Route path=\"/cart\" element={<Cart />} />\r\n              <Route path=\"/success\" element={<Success setPage={setPage}/>} />\r\n            </Routes>\r\n          </div>\r\n          <Footer setPage={setPage} />\r\n        </div>\r\n      </StoreProvider>\r\n      </Router>\r\n    </ApolloProvider>\r\n  );\r\n}\r\n\r\nexport default App;\r\n","C:\\Users\\lacyn\\Bootcamp\\Projects\\flower-shop\\client\\src\\pages\\Home.js",[],"C:\\Users\\lacyn\\Bootcamp\\Projects\\flower-shop\\client\\src\\utils\\GlobalState.js",[],"C:\\Users\\lacyn\\Bootcamp\\Projects\\fl